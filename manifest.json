{
    "manifest_version": 3,
    "name": "Force English Microsoft Docs",
    "version": "0.1.0",
    "description": "An extension to stop Microsoft Docs from being served in something other than US English. Localized MS Docs are often automatically and poorly translated. This extension will force US English Microsoft Docs.",
    "icons": {
        "16": "icons/stopmsdocs16x16.png",
        "32": "icons/stopmsdocs32x32.png",
        "48": "icons/stopmsdocs48x48.png",
        "128": "icons/stopmsdocs128x128.png",
        "256": "icons/stopmsdocs256x256.png",
        "512": "icons/stopmsdocs512x512.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/stopmsdocs16x16.png",
            "32": "icons/stopmsdocs32x32.png",
            "48": "icons/stopmsdocs48x48.png"
        },
        "default_title": "English MS Docs are forced"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["webNavigation"],
    "author": "D. Kreeft"
}
