{
    "manifest_version": 3,
    "name": "Force English Microsoft Docs",
    "version": "1.0.0",
    "description": "Extension to force English Microsoft Docs as they are often automatically served in a poorly translated local language.",
    "icons": {
        "16": "icons/stopmsdocs16x16.png",
        "32": "icons/stopmsdocs32x32.png",
        "48": "icons/stopmsdocs48x48.png",
        "128": "icons/stopmsdocs128x128.png",
        "256": "icons/stopmsdocs256x256.png",
        "512": "icons/stopmsdocs512x512.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/stopmsdocs16x16.png",
            "32": "icons/stopmsdocs32x32.png",
            "48": "icons/stopmsdocs48x48.png"
        },
        "default_title": "English MS Docs are forced"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["webNavigation"],
    "author": "D. Kreeft"
}
